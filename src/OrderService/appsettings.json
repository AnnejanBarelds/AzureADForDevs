{
  "Authentication": {
    "AzureAD": {
      "AadInstance": "https://login.microsoftonline.com/{0}",
      "Tenant": "[AD_TenantName]",
      "ClientId": "[OrderService_ClientID]",
      "ClientSecret": "[OrderService_ClientSecret]",
      "InventoryServiceResourceId": "https://[AD_TenantName]/InventoryService",
      "Audience": "https://[AD_TenantName]/OrderService"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
